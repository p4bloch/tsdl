{
  "name": "tsdl",
  "version": "0.0.1",
  "homepage": "https://github.com/p4bloch/tsdl",
  "description": "CLI tool para descargar subtitulos de tusubtitulo.com",
  "scripts": {
    "start": "nodemon --exec babel-node src/index.js",
    "build": "babel src --out-dir lib",
    "test": "export NODE_PATH=./ && mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.17.0",
    "babel-eslint": "6.1.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb-base": "^5.0.2",
    "eslint-plugin-import": "^1.16.0",
    "expect": "^1.20.1",
    "mocha": "^2.5.3",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "fuzzy": "^0.1.1",
    "inquirer": "^1.2.1",
    "mz": "^2.4.0",
    "request": "^2.69.0",
    "slug": "^0.9.1",
    "x-ray": "2.3.0"
  },
  "bin": {
    "tsdl": "bin/tsdl"
  },
  "engines": {
    "node": ">= 4"
  },
  "preferGlobal": true,
  "keywords": [],
  "author": {
    "name": "p4bloch"
  },
  "licenses": [
    {
      "type": "MIT"
    }
  ],
  "keywords": ["subtitulos", "tusubtitulo"]
}
